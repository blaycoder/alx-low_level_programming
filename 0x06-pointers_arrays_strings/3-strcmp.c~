#include "main.h"
/**
* _strcmp -function that compares two strings
* @s1: the first string
* @s2: the second string
* Return: Positive integer if s1 > s2, negative integer if s1 < s2 and Zero if s1 = s2
*/

int _strcmp(char *s1, char *s2)
{
int i = 0;
int j = 0;
while (s1[i] && s2[j] != '\0')
{
i++;
j++;
}
if (s1[i] == s2[j])
return (0);
else if (s1[i] > s2[j]
return (1);
else if (s1[i] < s2[j]
return (-1);
}
